<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wechat.qrcode.dao.InquireDao" >
 
	<select id="findByCode" resultType="com.wechat.qrcode.entity.InquireVO">
		select count(id) as count,code from inquire where code = #{code} group by code ;
	</select>
	
	<insert id="save">
		insert into inquire(code,createTime,openid) values(#{code},now(),#{openid});
	</insert>
</mapper>












