<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wechat.qrcode.dao.ProductDao" >

	<select id="findByCode" resultType="com.wechat.qrcode.entity.ProductVO">
		select * from product where code = #{code};
	</select>
	
	<select id="findAll" resultType="com.wechat.qrcode.entity.ProductVO">
		select * from product;
	</select>
</mapper>












